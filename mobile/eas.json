      },
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://sidekick-backend-qyrs.onrender.com",
        "JAVA_HOME": "/usr/lib/jvm/java-17-openjdk-amd64"
      },
      "prebuildCommand": "prebuild"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://sidekick-backend-qyrs.onrender.com",
        "JAVA_HOME": "/usr/lib/jvm/java-17-openjdk-amd64"
      },
      "prebuildCommand": "prebuild"
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "autoIncrement": true,
      "distribution": "store",
